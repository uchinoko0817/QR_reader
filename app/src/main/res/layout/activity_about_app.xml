<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView_title_version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Version"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/textView_title_developerName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView_contents_version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/textView_title_version"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView_title_developerName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Developer"
        app:layout_constraintTop_toTopOf="@id/textView_title_version"
        app:layout_constraintBottom_toTopOf="@id/textView_licence"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView_contents_developerName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Yutaro Uchino"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/textView_title_developerName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView_licence"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Licence"
        android:clickable="true"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/textView_title_developerName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>